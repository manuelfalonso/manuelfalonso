    "build": {
        "content": [
            {
                "src": "obj/temp/api",
                "files": [ "**/*.yml" ],
                "dest": "api"
            },
            {
                "files": [
                    "manual/**.md",
                    "manual/**/toc.yml",
                    "toc.yml",
                    "*.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "overwrite": [
            {
                "files": [
                    "apidoc/**.md"
                ],
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "globalMetadata": {
            "_appLogoPath": "images/icon.svg",
            "_appFooter": "Â© 2024 Sombra Studios",
            "_gitContribute": {                
                "repo": "https://github.com/manuelfalonso/Unity-Scripts",
                "branch": "master",
                "apiSpecFolder": "docs-ref-overwrite"
            }
        },
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "template"
        ],
        "postProcessors": [],
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false
    }
}
